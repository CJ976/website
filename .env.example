# Hackshala Environment Configuration Template
# Copy this file to .env and fill in the actual values
# IMPORTANT: Never commit .env file with actual secrets to version control

# === Web Application ===
# Discord Webhook URLs for form submissions
# Get these from your Discord server settings
VITE_REGISTRATION_WEBHOOK=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN
VITE_FEEDBACK_WEBHOOK=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# === Discord Bot ===
# Bot token from Discord Developer Portal
DISCORD_TOKEN=YOUR_BOT_TOKEN_HERE

# Role names for the Discord bot
PREMIUM_ROLE_NAME=Premium
ADMIN_ROLE_NAME=Admin

# === Security ===
# Enable/disable various security features (for future use)
ENABLE_RATE_LIMITING=true
ENABLE_AUDIT_LOGGING=true
